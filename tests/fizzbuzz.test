# A Fizz-Buzz test

def main()
{
	var end = 100;
	var iter = 1;

	println "----";
	println "FIZZ-BUZZ";
	println "----";


	while iter <= end
	{
		if (iter % 3 == 0) and (iter % 5 == 0)
		{
			print "FizzBuzz ";
		}
		elif iter % 3 == 0
		{
			print "Fizz ";
		}
		elif iter % 5 == 0
		{
			print "Buzz ";
		}
		else
		{
			print iter ++ " ";
		};
		iter = iter + 1;
	};
	println "";

	println "----";
}